  <sql id="val7">#{q4}</sql>
  <sql id="val8">#{q5}</sql>
  <sql id="val9">#{q6}</sql>
  <sql id="val10">#{q7}</sql>
  <sql id="val11">#{q8}</sql>
  <sql id="val12">#{q9}</sql>
  <sql id="val13">#{q10}</sql>

  <sql id="eqn0"><include refid="attr0"/>=<include refid="val0"/></sql>

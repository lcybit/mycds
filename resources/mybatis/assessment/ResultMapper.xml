  <sql id="attr12">q9</sql>
  <sql id="attr13">q10</sql>

  <sql id="val0">#{resultId}</sql>
  <sql id="val1">#{questionnaireId}</sql>
  <sql id="val2">#{sessionId}</sql>
  <sql id="val3">#{q0}</sql>

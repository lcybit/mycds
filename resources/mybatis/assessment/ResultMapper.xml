  </sql>
  <sql id="eqnQuestionnaireId">
    <include refid="eqn1"/>
  </sql>
  <sql id="eqnSessionId">
    <include refid="eqn2"/>
  </sql>

  <insert id="insert" parameterType="Result">
    INSERT INTO
    <include refid="table"/>
    (
    <include refid="attrAll"/>
    ) VALUES (
    <include refid="valAll"/>
    )
  </insert>

  <delete id="delete" parameterType="String">
    DELETE FROM
    <include refid="table"/>
    WHERE
    <include refid="eqnWhere"/>
  </delete>

  <delete id="batchDelete" parameterType="List">
    DELETE FROM
    <include refid="table"/>
    WHERE
    <include refid="attrPrimary"/>
    IN
    <foreach item="item" index="index" collection="list" open="(" separator="," close=")">#{item}</foreach>
  </delete>

  <update id="update" parameterType="Result">
    UPDATE
    <include refid="table"/>
    SET
    <include refid="eqnAll"/>
    WHERE
    <include refid="eqnWhere"/>
  </update>

  <select id="selectOne" parameterType="String" resultType="Result">
    SELECT
    <include refid="attrAll"/>
    FROM
    <include refid="table"/>
